<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.biz.platform.web.mapper.OrderMapper">

    <resultMap type="com.biz.platform.web.pojo.Order" id="orderResultMap">
        <result column="order_id" property="orderId"/>
        <result column="goods_id" property="goodsId"/>
        <result column="goods_name" property="goodsName"/>
        <result column="goods_logo_path" property="goodsLogoPath"/>
        <result column="goods_num" property="goodsNum"/>
        <result column="goods_price" property="goodsPrice"/>
        <result column="order_total_price" property="orderTotalPrice"/>
        <result column="order_buyer_id" property="orderBuyerId"/>
        <result column="order_buyer_name" property="orderBuyerName"/>
        <result column="order_buyer_statu" property="orderBuyerStatu"/>
        <result column="buyer_cancel_msg" property="buyerCancelMsg"/>
        <result column="order_shop_id" property="orderShopId"/>
        <result column="order_shop_name" property="orderShopName"/>
        <result column="order_shop_statu" property="orderShopStatu"/>
        <result column="shop_cancel_msg" property="shopCancelMsg"/>
        <result column="order_statu" property="orderStatu"/>
        <result column="order_is_pay" property="orderIsPay"/>
        <result column="delivery_start_addr" property="deliveryStartAddr"/>
        <result column="delivery_end_addr" property="deliveryEndAddr"/>
        <result column="logistics" property="logistics"/>
        <result column="create_time" property="createTime"/>
        <result column="last_modify_time" property="lastModifyTime"/>
    </resultMap>

    <sql id="sys_order_result">
        order_id,goods_id,goods_name,goods_logo_path,goods_num,goods_price,order_total_price,order_buyer_id,order_buyer_name,order_buyer_statu,
        buyer_cancel_msg,order_shop_id,order_shop_name,order_shop_statu,shop_cancel_msg,order_statu,order_is_pay,
        delivery_start_addr,delivery_end_addr,logistics,create_time,last_modify_time
    </sql>




</mapper>